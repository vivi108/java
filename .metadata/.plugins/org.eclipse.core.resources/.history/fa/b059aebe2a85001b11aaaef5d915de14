package Ex2_1b;
import java.util.Scanner;
public class Ex2_1b {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner keyboard = new Scanner(System.in);
		int fahren;
		float cel;
		fahren = keyboard.nextInt();
		cel = 5 *(fahren - 32)/9;
		
		System.out.println("Celsius is "+ cel);

	}

}
